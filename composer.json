        }
    ],
    "require": {
        "mindplay/annotations": "^1.3",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Controller\\": "./src/controllers",
            "Model\\Entity\\" : "./src/models/entities",
            "Core\\" : "./core", 
            "Core\\Model\\" : "./core/model",
            "Core\\Router\\" : "./core/router",
            "Core\\Model\\Converters\\" : "./core/model/converters",
            "mindplay\\annotations\\" : "./core/vendor/mindplay/annotations/src/annotations",
            "Dotenv\\" : "./core/vendor/vlucas/phpdotenv/src",
            "Core\\model\\Annotations\\" :[ "./core/model/annotations", "./core/model/annotations/annotation"],
            "Core\\Controller\\" : "./core/controller",
            "Core\\View\\Template\\" : "./core/view/template"
        }
    }, 
    "config": {
        "vendor-dir": "./core/vendor/"
    }
}
